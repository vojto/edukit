<h1>New subject_lesson</h1>

<% form_for(@subject_lesson) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :day %><br />
    <%= f.text_field :day %>
  </p>
  <p>
    <%= f.label :lesson %><br />
    <%= f.text_field :lesson %>
  </p>
  <p>
    <%= f.submit "Create" %>
  </p>
<% end %>

<%= link_to 'Back', subject_lessons_path %>
