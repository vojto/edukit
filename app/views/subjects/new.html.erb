<h1 class="clearfix">
	<div class="fl">
		Prida큐 predmet
	</div><!-- .fl -->
	<div class="fr">
		<%= link_to 'Sp채큐', subjects_path, :class => "normal_size" %>
	</div><!-- .fr -->
	
</h1>

<div class="content">
	<% form_for(@subject, :html => {:class => "normal"}) do |f| %>
	  <%= f.error_messages %>

	  <p>
	    <%= f.label :title %><br />
	    <%= f.text_field :title, :class => "text" %>
	  </p>
	  <p>
	    <%= f.submit "Prida큐" %>
	  </p>
	<% end %>

	
	
</div><!-- .content -->
